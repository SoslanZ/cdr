<?xml version="1.0" encoding="utf8" ?>
<project name="cdr" default="compile" basedir=".">
    <property file="${basedir}/build/project.properties" />

    <target name="backup" description="Backup project">
        <exec executable="php.bat">
            <arg path="${project.bin.dir}/staticfile.php" />
        </exec>
    </target>

    <target name="install" description="Install project for backup version">
        <exec executable="php-test.bat">
            <arg value="--output" />
            <arg path="${t}" />
        </exec>
    </target>

    <target name="compile" description="Prepare for build">
        <exec executable="phpab.bat">
            <arg value="--output" />
            <arg path="protected/autoload.php" />
            <arg path="protected" />
        </exec>
    </target>

</project>
